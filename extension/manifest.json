{
    "manifest_version": 3,
    "name": "Экзаминационные тесты полицеальных школ на русском - egzaminzawodowy.info",
    "version": "1.0.0",
    "description": "Польские тесты (вопросы, ответы и пояснения) профессионально переведены на русский, переводы появятся рядом с оригиналами. Убрана вся реклама с сайта testy.egzaminzawodowy.info.",
    "permissions": [],
    "content_scripts": [
        {
            "matches": [
                "https://testy.egzaminzawodowy.info/*",
                "https://www.testy.egzaminzawodowy.info/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
            "resources": [],
            "matches": [
                "https://testy.egzaminzawodowy.info/*",
                "https://www.testy.egzaminzawodowy.info/*"
            ]
        }
    ],
    "host_permissions": [
        "http://145.239.80.201:8081/*",
        "https://testy.egzaminzawodowy.info/*",
        "https://www.testy.egzaminzawodowy.info/*"
    ]
}
